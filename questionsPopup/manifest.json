{
  "manifest_version" : 2,
  "name" : "CP Extension",
  "version" : "0.1",
  "content_scripts" : [
    {
      "matches" : [
        "<all_urls>"
      ],
      "js" : ["content.js"]
    }
  ],
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action" : {
    "default_icon" : "titleimage.png",
    "default_popup" : "popup.html",
    "default_title" : "Questions Popup"
  },
  "permissions" : [
    "tabs",
    "notifications"
  ]
}
